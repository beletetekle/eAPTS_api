{
  "name": "InternalUser",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "username": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "roles": {
      "type": "hasMany",
      "model": "roleMapping",
      "foreignKey": "principalId"
    },
    "regions": {
      "type": "hasMany",
      "model": "Region",
      "foreignKey": "adminId"
    },
    "zones": {
      "type": "hasMany",
      "model": "Zone",
      "foreignKey": "adminId"
    },
    "woredas": {
      "type": "hasMany",
      "model": "Woreda",
      "foreignKey": "adminId"
    },
    "healthFacilities": {
      "type": "hasMany",
      "model": "HealthFacility",
      "foreignKey": "adminId"
    },
    "referer": {
      "type": "belongsTo",
      "model": "InternalUser",
      "foreignKey": "refererId"
    },
    "medicineBins": {
      "type": "hasMany",
      "model": "MedicineBin",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": [
        "__get__regions"
      ]
    }
  ],
  "methods": {}
}
